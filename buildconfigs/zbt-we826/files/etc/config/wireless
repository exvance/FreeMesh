config wifi-device  radio0
	option type     mac80211
	option channel  11
	option hwmode	11g
	option path	'platform/10180000.wmac'
	option htmode	HT20
	# REMOVE THIS LINE TO ENABLE WIFI:
	#option disabled 1

config wifi-iface
	option device   radio0
	option network  mesh
	option mode     adhoc
	option ssid     FreeMesh-Backhaul
	option bssid	'02:ca:ff:ee:ba:be'
	option encryption none

config wifi-device mt7612e
	option type mt7612e
	option vendor ralink
	option band 5G
	option channel 0
	option autoch 2

config wifi-iface
	option device mt7612e
	option ifname rai0
	option network meshbridge
	option mode ap
	option ssid FreeMesh
	option encryption psk2
	option key FreeMesh
